{
  "editor.minimap.enabled": false,
  "terminal.integrated.fontWeight": "1",
  "editor.fontFamily": "Consolas, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.fontWeight": "1",
  "workbench.editor.showTabs": true,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
      "editorCursor.foreground": "#00ff33",
      "editor.lineHighlightBackground": "#111144"
  },
  "workbench.statusBar.visible": true,
  "editor.lineNumbers": "on",
  "C_Cpp.clang_format_style": "{ BasedOnStyle: GNU, IndentWidth: 2, ColumnLimit: 0, NamespaceIndentation: All}",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
  "C_Cpp.vcFormat.space.pointerReferenceAlignment": "left",
  "C_Cpp.clang_format_sortIncludes": false,
  "C_Cpp.vcFormat.indent.namespaceContents": true,
  "editor.cursorStyle": "line-thin",
  "window.zoomLevel": 0,
  "editor.tabCompletion": "on",
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
      {
          "text": "TODO:",
          "color": "red",
          "backgroundColor": "rgba(0,0,0,0)"
      },
      {
          "text": "HACK:",
          "color": "#ff6600",
          "backgroundColor": "rgba(0,0,0,0)"
      },
      {
          "text": "NOTE:",
          "color": "green",
          "backgroundColor": "rgba(0,0,0,0)"
      }
  ],
  "todohighlight.defaultStyle": {
      "color": "red",
      "backgroundColor": "#ffab00",
      "overviewRulerColor": "rgba(0,0,0,0)",
      "isWholeLine": false,
  },
  "files.associations": {
      "test.h": "c",
      "vulkan.h": "c",
      "vulkan_core.h": "c",
      "vk_platform.h": "c",
      "type_traits": "c",
      "typeinfo": "c",
      "windowsx.h": "c",
      "vulkan_platform.h": "c",
      "renderer_frontend.h": "c"
  }
}